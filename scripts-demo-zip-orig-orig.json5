{
  base_dir: './demo',
  version: '2.5.23',
  execute: {
    action: 'sequential',
    actions: [
      {
        action: 'parallel',
        actions: [
          {
            action: 'build',
            config: {
              base_dir: './demo',
              env: {
                CONFIG_ENV: 'top100',
              },
            },
          },
          {
            action: 'build',
            config: {
              base_dir: './demo',
              env: {
                CONFIG_ENV: 'stats-top100',
              }
            },
          },
        ],
      },
      {
        action: 'version',
        config: {
          release: 'patch',
        },
      },
      {
        action: 'zip',
        config: {
          file_names: [
            './dist/',
          ],
          archive_prefix: 'dist',
          out_dir: '../_archive',
          exclude_files: [
            'index.html',
            'index.js',
            'minimal.html',
            'fp.ico',
            'fp.png',
            'ldr.js.LICENSE.txt',
          ],
        },
      },
    ],
  },
}
