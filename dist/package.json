    "license": "MIT",
    "bugs": {
        "url": "https://github.com/alykoshin/tools-runner/issues"
    },
    "homepage": "https://github.com/bcherny/json-schema-to-typescript#readme",
    "dependencies": {
        "@types/ejs": "^3.1.2",
        "@types/lodash": "^4.14.199",
        "@types/node": "^20.6.3",
        "@types/semver": "^7.5.2",
        "@types/shelljs": "^0.8.12",
        "@utilities/object": "^0.4.3",
        "ajv": "^8.12.0",
        "chalk": "4.1.2",
        "commander": "^11.0.0",
        "ejs": "^3.1.9",
        "git-config": "^0.0.7",
        "json5": "^2.2.3",
        "lodash": "^4.17.21",
        "octokit": "^3.1.0",
        "semver": "^7.5.4",
        "shelljs": "^0.8.5",
        "shx": "^0.3.4",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
    },
